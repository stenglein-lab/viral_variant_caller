
process {

  // conda = "environment_setup/variant_conda_environment.yaml"
  conda = "$HOME/variant_conda_environment"

  withLabel: 'highmem' {
    maxForks = 4
	 cpus = 12
  }

  withLabel: 'lowmem_threaded' {
    maxForks = 6
	 cpus = 8
  }

  withLabel: 'lowmem_non_threaded' {
    maxForks = 24
	 cpus = 1
  }
}

